{
    "manifest_version": 3,
    "name": "internet speed checker",
    "description": "cool internet speed checker extension",
    "version": "1.0",
    "action": {
        "default_popup": "index.html"
    },
    "icons": {
        "16": "icons/favicon-16x16.png",
        "48": "icons/favicon-48x48.png",
        "128": "icons/favicon-128x128.png"
    },
    "permissions": [
        "storage"
    ]
}